<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="szene" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="jrp.mietmanager.benutzeroberflaeche.nebenfenster.StartansichtController" stylesheets="@../JRPThemeDark.css">
   <children>
      <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="400.0">
         <children>
            <Label text="Applogo und Apptitel" styleClass="title-label">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Label>
         </children>
      </HBox>
      <HBox>
         <children>
            <Label text="Zuletzt verwendete Immobilien:">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Label>
         </children>
      </HBox>
      <VBox>
         <children>
            <Label text="Immobilie 1 /Dateipfad/beispiel/test">
               <VBox.margin>
                  <Insets bottom="5.0" left="50.0" right="10.0" top="5.0" />
               </VBox.margin>
            </Label>
            <Label text="Immobilie 2 /Dateipfad/beispiel/test">
               <VBox.margin>
                  <Insets bottom="5.0" left="50.0" right="10.0" top="5.0" />
               </VBox.margin>
            </Label>
            <Label text="Immobilie 3 /Dateipfad/beispiel/test">
               <VBox.margin>
                  <Insets bottom="5.0" left="50.0" right="10.0" top="5.0" />
               </VBox.margin>
            </Label>
         </children>
      </VBox>
      <HBox alignment="BOTTOM_RIGHT">
         <children>
            <Button fx:id="neueImmobilieTaster" mnemonicParsing="false" onAction="#neueImmobilieErstellen" text="Neue Immobilie erstellen">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
</VBox>
